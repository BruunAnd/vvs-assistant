<ContentControl x:Class="VVSAssistant.Views.PdfCalculationLayout"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:VVSAssistant.Views"
             xmlns:viewModels="clr-namespace:VVSAssistant.ViewModels"
             mc:Ignorable="d" 
             d:DesignHeight="1200" d:DesignWidth="1000">
    <ContentControl.DataContext>
        <viewModels:PdfCalculationViewModel/>
    </ContentControl.DataContext>

    <Grid Height="27.7cm" Width="19cm" Background="White">
        <Grid.RowDefinitions>
            <RowDefinition Height="27.7cm" />
        </Grid.RowDefinitions>


        <!--Page 1-->
        <Grid Grid.Column="0" Grid.Row="0" Height="27.7cm" Width="19.0cm" Margin="0,0,0.11,0" Visibility="{Binding PageOne}">
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="50"/>
                <ColumnDefinition Width="1*"/>
                <ColumnDefinition Width="110"/>
                <ColumnDefinition Width="50"/>
            </Grid.ColumnDefinitions>
            <Grid.RowDefinitions>
                <RowDefinition Height="70"/>
                <RowDefinition Height="70"/>
                <RowDefinition Height="70"/>
                <RowDefinition Height="70"/>
                <RowDefinition Height="120"/>
                <RowDefinition Height="70"/>
                <RowDefinition Height="110"/>
                <RowDefinition Height="auto"/>
                <RowDefinition Height="1*"/>
                <RowDefinition Height="auto"/>
            </Grid.RowDefinitions>
            <Label Grid.Column="1" Grid.Row="1" VerticalAlignment="Center" Margin="0,0,0,-20" FontSize="12" Content="Årsvirkningsgrad ved rumopvarmning for varmepumpe"/>

            <Canvas Grid.Column="1" Grid.Row="1" HorizontalAlignment="Right">
                <Grid>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="1*"/>
                        <ColumnDefinition Width="1*"/>
                        <ColumnDefinition Width="1*"/>
                    </Grid.ColumnDefinitions>
                    <Grid.RowDefinitions>
                        <RowDefinition Height="auto"/>
                        <RowDefinition Height="auto"/>
                    </Grid.RowDefinitions>
                    <Ellipse Grid.Column="1" Grid.Row="0" VerticalAlignment="Bottom" Margin="0,0,0,-3" Fill="Cyan" Height="15" Width="15"/>
                    <Label Grid.Column="1" Grid.Row="0" HorizontalAlignment="Center" VerticalAlignment="Bottom" Margin="0,0,0,-9" FontSize="14" FontWeight="Bold" Foreground="White" Content="1" />
                    <Label Grid.Column="0" Grid.Row="1" VerticalAlignment="Center" Margin="0,0,0,5" FontSize="26" FontWeight="Bold" Content="   " Foreground="Cyan"/>
                    <Border Grid.Column="1" Grid.Row="1" Width="1.5cm" Height="1cm" BorderThickness="2" BorderBrush="Cyan"/>
                    <Label Grid.Column="1" Grid.Row="1" VerticalAlignment="Center" HorizontalAlignment="Center" FontSize="16" FontWeight="Bold" Content="{Binding PrimAnnualEfficiency}" />
                    <Label Grid.Column="2" Grid.Row="1" FontSize="20" FontWeight= "Bold" Content="%" Foreground="Cyan"/>
                </Grid>
            </Canvas>
            <Border Grid.Column="1" Grid.Row="1" Grid.ColumnSpan="2" Width="auto" Height="1" VerticalAlignment="Bottom" Background="Black" />


            <Grid Grid.Column="1" Grid.Row="2">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="1*" />
                    <ColumnDefinition Width="270" />
                </Grid.ColumnDefinitions>
                <Grid.RowDefinitions>
                    <RowDefinition Height="1*" />
                    <RowDefinition Height="1*" />
                </Grid.RowDefinitions>
                <Label Grid.Column="0" Grid.Row="0" FontSize="12" Content="Temperaturstyring"/>
                <Label Grid.Column="0" Grid.Row="1" FontSize="12" Content="Fra temperaturstyrings datablad"/>
                <Border Grid.Column="1" Grid.Row="0" Grid.RowSpan="2" BorderBrush="Cyan" BorderThickness="1.5" Background="White" CornerRadius="8" Margin="5,5,5,5"/>
                <TextBlock Grid.Column="1" Grid.Row="0" Grid.RowSpan="2" Margin="8,8,8,8" FontSize="12">
                    Klasse I = 1 %, Klasse II = 2 %. Klasse III = 1,5 %, <LineBreak/>
                    Klasse IV = 2 %, Klasse V = 3 %, Klasse VI = 4 %, <LineBreak/>
                    Klasse VII = 3,5 %, Klasse VIII = 5 %
                </TextBlock>
            </Grid>

            <Canvas Grid.Column="1" Grid.Row="2" HorizontalAlignment="Right">
                <Grid>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="1*"/>
                        <ColumnDefinition Width="1*"/>
                        <ColumnDefinition Width="1*"/>
                    </Grid.ColumnDefinitions>
                    <Grid.RowDefinitions>
                        <RowDefinition Height="auto"/>
                        <RowDefinition Height="auto"/>
                    </Grid.RowDefinitions>
                    <Ellipse Grid.Column="1" Grid.Row="0" VerticalAlignment="Bottom" Margin="0,0,0,-3" Fill="Cyan" Height="15" Width="15"/>
                    <Label Grid.Column="1" Grid.Row="0" HorizontalAlignment="Center" VerticalAlignment="Bottom" Margin="0,0,0,-9" FontSize="14" FontWeight="Bold" Foreground="White" Content="2" />
                    <Label Grid.Column="0" Grid.Row="1" VerticalAlignment="Center" Margin="0,0,0,5" FontSize="26" FontWeight="Bold" Content="+" Foreground="Cyan"/>
                    <Border Grid.Column="1" Grid.Row="1" Width="1.5cm" Height="1cm" BorderThickness="2" BorderBrush="Cyan"/>
                    <Label Grid.Column="1" Grid.Row="1" VerticalAlignment="Center" HorizontalAlignment="Center" FontSize="16" FontWeight="Bold" Content="{Binding TemperatureControleclass}" />
                    <Label Grid.Column="2" Grid.Row="1" FontSize="20" FontWeight= "Bold" Content="%" Foreground="Cyan"/>
                </Grid>
            </Canvas>
            <Border Grid.Column="1" Grid.Row="2" Grid.ColumnSpan="2" Width="auto" Height="1" VerticalAlignment="Bottom" Background="Black" />


            <Grid Grid.Column="1" Grid.Row="3">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="auto"/>
                    <ColumnDefinition Width="1*" />
                    <ColumnDefinition Width="1*" />
                    <ColumnDefinition Width="auto" />
                    <ColumnDefinition Width="auto" />
                </Grid.ColumnDefinitions>
                <Grid.RowDefinitions>
                    <RowDefinition Height="1*" />
                    <RowDefinition Height="1*" />
                </Grid.RowDefinitions>
                <Label Grid.Column="0" Grid.Row="0" FontSize="12" Content="Supplerende kedel" />
                <Label Grid.Column="0" Grid.Row="1" FontSize="12" Content="Fra kedlens datablad" />
                <Border Grid.Column="2" Grid.Row="0" Grid.ColumnSpan="3" BorderBrush="Cyan" BorderThickness="1.5" CornerRadius="8" Margin="5,5,5,5" />
                <Label Grid.Column="2" Grid.Row="0" Grid.ColumnSpan="3" HorizontalAlignment="Center" VerticalAlignment="Center" FontSize="12" Content="Årsvirkningsgrad ved rumopvarmning (i %)" />
                <Label Grid.Column="2" Grid.Row="1" HorizontalAlignment="Right" FontSize="12" Margin="0,0,0,0" Content="(     " />
                <Label Grid.Column="3" Grid.Row="1" VerticalAlignment="Top" HorizontalAlignment="Center" FontSize="15" Margin="0,-15,0,0" Content="|"/>
                <Border Grid.Column="3" Grid.Row="1" Width="0.75cm" Height="0.4cm" VerticalAlignment="Center" BorderBrush="Black" BorderThickness="1" Margin=" 5,5,5,9"/>
                <Label Grid.Column="3" Grid.Row="1" FontSize="10" Content="{Binding SupBoilerAnnualEfficiency}" VerticalAlignment="Center" HorizontalAlignment="Center" Margin="6,3,6,11" />

                <Label Grid.Column="4" Grid.Row="1" FontSize="12" Content="     -     'I'     )     x     'II'   =" />
            </Grid>

            <Canvas Grid.Column="1" Grid.Row="3" HorizontalAlignment="Right">
                <Grid>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="1*"/>
                        <ColumnDefinition Width="1*"/>
                        <ColumnDefinition Width="1*"/>
                    </Grid.ColumnDefinitions>
                    <Grid.RowDefinitions>
                        <RowDefinition Height="auto"/>
                        <RowDefinition Height="auto"/>
                    </Grid.RowDefinitions>
                    <Ellipse Grid.Column="1" Grid.Row="0" VerticalAlignment="Bottom" Margin="0,0,0,-3" Fill="Cyan" Height="15" Width="15"/>
                    <Label Grid.Column="1" Grid.Row="0" HorizontalAlignment="Center" VerticalAlignment="Bottom" Margin="0,0,0,-9" FontSize="14" FontWeight="Bold" Foreground="White" Content="3" />
                    <Label Grid.Column="0" Grid.Row="1" VerticalAlignment="Center" Margin="0,0,0,5" FontSize="26" FontWeight="Bold" Content=" -" Foreground="Cyan"/>
                    <Border Grid.Column="1" Grid.Row="1" Width="1.5cm" Height="1cm" BorderThickness="2" BorderBrush="Cyan"/>
                    <Label Grid.Column="1" Grid.Row="1" VerticalAlignment="Center" HorizontalAlignment="Center" FontSize="16" FontWeight="Bold" Content="{Binding SupBoilerTotal}" />
                    <Label Grid.Column="2" Grid.Row="1" FontSize="20" FontWeight= "Bold" Content="%" Foreground="Cyan"/>
                </Grid>
            </Canvas>
            <Border Grid.Column="1" Grid.Row="3" Grid.ColumnSpan="2" Width="auto" Height="1" VerticalAlignment="Bottom" Background="Black" />


            <Grid Grid.Column="1" Grid.Row="4">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="auto" />
                    <ColumnDefinition Width="auto" />
                    <ColumnDefinition Width="auto" />
                    <ColumnDefinition Width="auto" />
                    <ColumnDefinition Width="auto" />
                    <ColumnDefinition Width="auto" />
                    <ColumnDefinition Width="auto" />
                    <ColumnDefinition Width="auto" />
                    <ColumnDefinition Width="auto" />
                </Grid.ColumnDefinitions>
                <Grid.RowDefinitions>
                    <RowDefinition Height="1*" />
                    <RowDefinition Height="40" />
                    <RowDefinition Height="30" />
                </Grid.RowDefinitions>
                <Label Grid.Column="0" Grid.Row="0" Grid.ColumnSpan="2" FontSize="12" VerticalAlignment="Top" Content="Solvarmebidrag" />
                <Label Grid.Column="0" Grid.Row="0" Grid.ColumnSpan="5" FontSize="12" VerticalAlignment="Bottom" Content="Fra solvarmekomponentens datablad" />
                <Border Grid.Column="0" Grid.Row="1" Width="100" Height="40" Grid.ColumnSpan="3" BorderBrush="Cyan" BorderThickness="1.5" CornerRadius="8" Margin="0,0,0,0" />
                <TextBlock Grid.Column="0" Grid.Row="1" Grid.ColumnSpan="3" LineHeight="15" FontSize="12" VerticalAlignment="Center" HorizontalAlignment="Center" Margin="5,5,5,5">
                    Solfangerens <LineBreak/>
                    størrelse (i <Run Text="m"></Run><Run Text="2" FontSize="7" BaselineAlignment="Superscript"/> )
                </TextBlock>
                <Border Grid.Column="2" Grid.Row="1" Width="110" Height="40" Grid.ColumnSpan="3" BorderBrush="Cyan" BorderThickness="1.5" CornerRadius="8" Margin="0,0,0,0" />
                <TextBlock Grid.Column="2" Grid.Row="1" Grid.ColumnSpan="3" LineHeight="15" FontSize="12" VerticalAlignment="Center" HorizontalAlignment="Center" Margin="5,5,5,5">
                    Beholderens <LineBreak/>
                    vandindhold (i <Run Text="m"></Run><Run Text="3" FontSize="7" BaselineAlignment="Superscript"/> )
                </TextBlock>
                <Border Grid.Column="4" Grid.Row="1" Width="100" Height="40" Grid.ColumnSpan="3" BorderBrush="Cyan" BorderThickness="1.5" CornerRadius="8" Margin="0,0,0,0" />
                <TextBlock Grid.Column="4" Grid.Row="1" Grid.ColumnSpan="3" HorizontalAlignment="Center" LineHeight="15" FontSize="12" VerticalAlignment="Center" Margin="5,5,5,5">
                    Solfangerens  <LineBreak/>
                    effektivitet (i %)
                </TextBlock>
                <Border Grid.Column="6" Grid.Row="0" Grid.ColumnSpan="3" Grid.RowSpan="2" Width="110" Height="85"  BorderBrush="Cyan" BorderThickness="1.5" CornerRadius="8" Margin="0,5,-20,0" />
                <TextBlock Grid.Column="6" Grid.Row="0" Grid.ColumnSpan="3" Grid.RowSpan="2" LineHeight="10" FontSize="12" VerticalAlignment="Center" HorizontalAlignment="Center" Margin="5,5,-16,5">
                    Beholderens <LineBreak/>
                    Klassificering <LineBreak/>
                    A* = 0,95, A = 0,91,<LineBreak/>
                    B = 0,86, C = 0,83, <LineBreak/>
                    D-G = 0,81
                </TextBlock>
                <Label Grid.Column="0" Grid.Row="2" FontSize="12" Content="(   'III'   x   "/>
                <Label Grid.Column="1" Grid.Row="2" FontWeight="Bold" VerticalAlignment="Top" HorizontalAlignment="Center" FontSize="8" Margin="0,-8,0,0" Content="|"/>
                <Border Grid.Column="1" Grid.Row="2" Width="0.75cm" Height="0.4cm" VerticalAlignment="Center" BorderBrush="Black" BorderThickness="1" Margin=" 5,5,5,9"/>
                <Label Grid.Column="1" Grid.Row="2" FontSize="10" VerticalAlignment="Center" HorizontalAlignment="Center" Content="{Binding SolarM2}" Margin="3,1,3,7" />
                <Label Grid.Column="2" Grid.Row="2" FontSize="12" Content="   +   'IV'   x   "/>
                <Label Grid.Column="3" Grid.Row="2" FontWeight="Bold" VerticalAlignment="Top" HorizontalAlignment="Center" FontSize="8" Margin="0,-8,0,0" Content="|"/>
                <Border Grid.Column="3" Grid.Row="2" Width="0.75cm" Height="0.4cm" VerticalAlignment="Center" BorderBrush="Black" BorderThickness="1" Margin=" 5,5,5,9"/>
                <Label Grid.Column="3" Grid.Row="2" FontSize="10" VerticalAlignment="Center" HorizontalAlignment="Center" Content="{Binding SolarM3}" Margin="5,1,5,0" Height="29" />
                <Label Grid.Column="4" Grid.Row="2" FontSize="12" Content="   )   x   0,45   x   (   "/>
                <Label Grid.Column="5" Grid.Row="2" FontWeight="Bold" VerticalAlignment="Top" HorizontalAlignment="Center" FontSize="8" Margin="0,-8,0,0" Content="|"/>
                <Border Grid.Column="5" Grid.Row="2" Width="0.75cm" Height="0.4cm" VerticalAlignment="Center" BorderBrush="Black" BorderThickness="1" Margin=" 5,5,5,9"/>
                <Label Grid.Column="5" Grid.Row="2" FontSize="10" VerticalAlignment="Center" HorizontalAlignment="Center" Content="{Binding SolarEfficiency}" Margin="3,1,3,7" />
                <Label Grid.Column="6" Grid.Row="2" FontSize="12" Content="    /100   )   x   "/>
                <Label Grid.Column="7" Grid.Row="2" FontWeight="Bold" VerticalAlignment="Top" HorizontalAlignment="Center" FontSize="8" Margin="0,-8,0,0" Content="|"/>
                <Border Grid.Column="7" Grid.Row="2" Width="0.75cm" Height="0.4cm" VerticalAlignment="Center" BorderBrush="Black" BorderThickness="1" Margin=" 5,5,5,9"/>
                <Label Grid.Column="7" Grid.Row="2" FontSize="10" VerticalAlignment="Center" HorizontalAlignment="Center" Content="{Binding SolarClass}" Margin="3,1,3,7" />
                <Label Grid.Column="8" Grid.Row="2" FontSize="12" Content="  =  "/>
            </Grid>

            <Canvas Grid.Column="1" Grid.Row="4" HorizontalAlignment="Right" VerticalAlignment="Center" Margin="0,-30,0,0">
                <Grid>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="1*"/>
                        <ColumnDefinition Width="1*"/>
                        <ColumnDefinition Width="1*"/>
                    </Grid.ColumnDefinitions>
                    <Grid.RowDefinitions>
                        <RowDefinition Height="auto"/>
                        <RowDefinition Height="auto"/>
                    </Grid.RowDefinitions>
                    <Ellipse Grid.Column="1" Grid.Row="0" VerticalAlignment="Bottom" Margin="0,0,0,-3" Fill="Cyan" Height="15" Width="15"/>
                    <Label Grid.Column="1" Grid.Row="0" HorizontalAlignment="Center" VerticalAlignment="Bottom" Margin="0,0,0,-9" FontSize="14" FontWeight="Bold" Foreground="White" Content="4" />
                    <Label Grid.Column="0" Grid.Row="1" VerticalAlignment="Center" Margin="0,0,0,5" FontSize="26" FontWeight="Bold" Content="+" Foreground="Cyan"/>
                    <Border Grid.Column="1" Grid.Row="1" Width="1.5cm" Height="1cm" BorderThickness="2" BorderBrush="Cyan"/>
                    <Label Grid.Column="1" Grid.Row="1" VerticalAlignment="Center" HorizontalAlignment="Center" FontSize="16" FontWeight="Bold" Content="{Binding SolarTotal}" />
                    <Label Grid.Column="2" Grid.Row="1" FontSize="20" FontWeight= "Bold" Content="%" Foreground="Cyan"/>
                </Grid>
            </Canvas>
            <Border Grid.Column="1" Grid.Row="4" Grid.ColumnSpan="2" Width="auto" Height="1" VerticalAlignment="Bottom" Background="Black" />


            <Label Grid.Column="1" Grid.Row="5" VerticalAlignment="Center" FontSize="12" Content="Pakkens årsvirkningsgrad ved opvarmning under gennemsnitlige klimaforhold"/>
            <Canvas Grid.Column="1" Grid.Row="5" HorizontalAlignment="Right">
                <Grid>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="1*"/>
                        <ColumnDefinition Width="1*"/>
                        <ColumnDefinition Width="1*"/>
                    </Grid.ColumnDefinitions>
                    <Grid.RowDefinitions>
                        <RowDefinition Height="auto"/>
                        <RowDefinition Height="auto"/>
                    </Grid.RowDefinitions>
                    <Ellipse Grid.Column="1" Grid.Row="0" VerticalAlignment="Bottom" Margin="0,0,0,-3" Fill="Cyan" Height="15" Width="15"/>
                    <Label Grid.Column="1" Grid.Row="0" HorizontalAlignment="Center" VerticalAlignment="Bottom" Margin="0,0,0,-9" FontSize="14" FontWeight="Bold" Foreground="White" Content="5" />
                    <Label Grid.Column="0" Grid.Row="1" VerticalAlignment="Center" Margin="0,0,0,5" FontSize="26" FontWeight="Bold" Content="   " Foreground="Cyan"/>
                    <Border Grid.Column="1" Grid.Row="1" Width="1.5cm" Height="1cm" BorderThickness="2" BorderBrush="Cyan"/>
                    <Label Grid.Column="1" Grid.Row="1" VerticalAlignment="Center" HorizontalAlignment="Center" FontSize="16" FontWeight="Bold" Content="{Binding PackagedAnnualEfficiencyAverageClima}" />
                    <Label Grid.Column="2" Grid.Row="1" FontSize="20" FontWeight= "Bold" Content="%" Foreground="Cyan"/>
                </Grid>
            </Canvas>
            <Border Grid.Column="1" Grid.Row="5" Grid.ColumnSpan="2" Width="auto" Height="1" VerticalAlignment="Bottom" Background="Black" />


            <Grid Grid.Column="1" Grid.Row="6" Grid.ColumnSpan="2" Margin="0,0,26,0">
                <Grid.RowDefinitions>
                    <RowDefinition Height="30" />
                    <RowDefinition Height="1*" />
                </Grid.RowDefinitions>
                <Label Grid.Column="0" Grid.Row="0" FontSize="12" Content="Pakkens klasse for årsvirkningsgrad ved rumopvarmning under gennemsnitlige klimaforhold" />
                <Border Grid.Column="0" Grid.Row="1" BorderBrush="Cyan" BorderThickness="1.5" CornerRadius="8" Margin="30,-5,30,5" />
                <Grid Grid.Column="0" Grid.Row="1">
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="35"/>
                        <ColumnDefinition Width="1*"/>
                        <ColumnDefinition Width="1*"/>
                        <ColumnDefinition Width="1*"/>
                        <ColumnDefinition Width="1*"/>
                        <ColumnDefinition Width="1*"/>
                        <ColumnDefinition Width="1*"/>
                        <ColumnDefinition Width="1*"/>
                        <ColumnDefinition Width="1*"/>
                        <ColumnDefinition Width="1*"/>
                        <ColumnDefinition Width="1*"/>
                        <ColumnDefinition Width="35"/>
                    </Grid.ColumnDefinitions>
                    <Grid.RowDefinitions>
                        <RowDefinition Height="20"/>
                        <RowDefinition Height="20"/>
                        <RowDefinition Height="1*"/>
                    </Grid.RowDefinitions>
                    <Border Grid.Column="1" Grid.Row="0" HorizontalAlignment="Center" VerticalAlignment="Center" Height="0.4cm" Width="0.4cm" BorderBrush="Black" BorderThickness="1"/>
                    <Canvas Grid.Column="1" Grid.Row="1">
                        <Grid>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="30"/>
                                <ColumnDefinition Width="20"/>
                            </Grid.ColumnDefinitions>
                            <Border Grid.Column="0" Grid.Row="0" Grid.ColumnSpan="2" Height="20" HorizontalAlignment="Right" VerticalAlignment="Center" Width="30" Background="black" Margin="0,1,10,-0.2" />
                            <Label HorizontalAlignment="Right" VerticalAlignment="Center" FontSize="14" Content="G" Foreground="White" Grid.ColumnSpan="2" Margin="0,-1,15,0.8"/>
                            <Polygon Grid.Row="0" HorizontalAlignment="Left" VerticalAlignment="Center" Margin="40,2,0,-0.2" Points="0,17.9,0,0,10,10" Fill="Black" Stroke="Black" Grid.ColumnSpan="2"/>
                        </Grid>
                    </Canvas>
                    <Canvas Grid.Column="2" Grid.Row="1">
                        <Grid>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="30"/>
                                <ColumnDefinition Width="20"/>
                            </Grid.ColumnDefinitions>
                            <Border Grid.Column="0" Grid.Row="0" Grid.ColumnSpan="2" Height="20" HorizontalAlignment="Right" VerticalAlignment="Center" Width="30" Background="black" Margin="0,1,10,-0.2" />
                            <Label HorizontalAlignment="Right" VerticalAlignment="Center" FontSize="14" Content="F" Foreground="White" Grid.ColumnSpan="2" Margin="0,-1,15,0.8"/>
                            <Polygon Grid.Row="0" HorizontalAlignment="Left" VerticalAlignment="Center" Margin="40,2,0,-0.2" Points="0,17.9,0,0,10,10" Fill="Black" Stroke="Black" Grid.ColumnSpan="2"/>
                        </Grid>
                    </Canvas>
                    <Canvas Grid.Column="3" Grid.Row="1">
                        <Grid>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="30"/>
                                <ColumnDefinition Width="20"/>
                            </Grid.ColumnDefinitions>
                            <Border Grid.Column="0" Grid.Row="0" Grid.ColumnSpan="2" Height="20" HorizontalAlignment="Right" VerticalAlignment="Center" Width="30" Background="black" Margin="0,1,10,-0.2" />
                            <Label HorizontalAlignment="Right" VerticalAlignment="Center" FontSize="14" Content="E" Foreground="White" Grid.ColumnSpan="2" Margin="0,-1,15,0.8"/>
                            <Polygon Grid.Row="0" HorizontalAlignment="Left" VerticalAlignment="Center" Margin="40,2,0,-0.2" Points="0,17.9,0,0,10,10" Fill="Black" Stroke="Black" Grid.ColumnSpan="2"/>
                        </Grid>
                    </Canvas>
                    <Canvas Grid.Column="4" Grid.Row="1">
                        <Grid>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="30"/>
                                <ColumnDefinition Width="20"/>
                            </Grid.ColumnDefinitions>
                            <Border Grid.Column="0" Grid.Row="0" Grid.ColumnSpan="2" Height="20" HorizontalAlignment="Right" VerticalAlignment="Center" Width="30" Background="black" Margin="0,1,10,-0.2" />
                            <Label HorizontalAlignment="Right" VerticalAlignment="Center" FontSize="14" Content="D" Foreground="White" Grid.ColumnSpan="2" Margin="0,-1,15,0.8"/>
                            <Polygon Grid.Row="0" HorizontalAlignment="Left" VerticalAlignment="Center" Margin="40,2,0,-0.2" Points="0,17.9,0,0,10,10" Fill="Black" Stroke="Black" Grid.ColumnSpan="2"/>
                        </Grid>
                    </Canvas>
                    <Canvas Grid.Column="5" Grid.Row="1">
                        <Grid>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="30"/>
                                <ColumnDefinition Width="20"/>
                            </Grid.ColumnDefinitions>
                            <Border Grid.Column="0" Grid.Row="0" Grid.ColumnSpan="2" Height="20" HorizontalAlignment="Right" VerticalAlignment="Center" Width="30" Background="black" Margin="0,1,10,-0.2" />
                            <Label HorizontalAlignment="Right" VerticalAlignment="Center" FontSize="14" Content="C" Foreground="White" Grid.ColumnSpan="2" Margin="0,-1,15,0.8"/>
                            <Polygon Grid.Row="0" HorizontalAlignment="Left" VerticalAlignment="Center" Margin="40,2,0,-0.2" Points="0,17.9,0,0,10,10" Fill="Black" Stroke="Black" Grid.ColumnSpan="2"/>
                        </Grid>
                    </Canvas>
                    <Canvas Grid.Column="6" Grid.Row="1">
                        <Grid>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="30"/>
                                <ColumnDefinition Width="20"/>
                            </Grid.ColumnDefinitions>
                            <Border Grid.Column="0" Grid.Row="0" Grid.ColumnSpan="2" Height="20" HorizontalAlignment="Right" VerticalAlignment="Center" Width="30" Background="black" Margin="0,1,10,-0.2" />
                            <Label HorizontalAlignment="Right" VerticalAlignment="Center" FontSize="14" Content="B" Foreground="White" Grid.ColumnSpan="2" Margin="0,-1,15,0.8"/>
                            <Polygon Grid.Row="0" HorizontalAlignment="Left" VerticalAlignment="Center" Margin="40,2,0,-0.2" Points="0,17.9,0,0,10,10" Fill="Black" Stroke="Black" Grid.ColumnSpan="2"/>
                        </Grid>
                    </Canvas>
                    <Canvas Grid.Column="7" Grid.Row="1">
                        <Grid>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="30"/>
                                <ColumnDefinition Width="20"/>
                            </Grid.ColumnDefinitions>
                            <Border Grid.Column="0" Grid.Row="0" Grid.ColumnSpan="2" Height="20" HorizontalAlignment="Right" VerticalAlignment="Center" Width="30" Background="black" Margin="0,1,10,-0.2" />
                            <Label HorizontalAlignment="Right" VerticalAlignment="Center" FontSize="14" Content="A" Foreground="White" Grid.ColumnSpan="2" Margin="0,-1,15,0.8" />
                            <Polygon Grid.Row="0" HorizontalAlignment="Left" VerticalAlignment="Center" Margin="40,2,0,-0.2" Points="0,17.9,0,0,10,10" Fill="Black" Stroke="Black" Grid.ColumnSpan="2"/>
                        </Grid>
                    </Canvas>
                    <Canvas Grid.Column="8" Grid.Row="1">
                        <Grid>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="30"/>
                                <ColumnDefinition Width="20"/>
                            </Grid.ColumnDefinitions>
                            <Border Grid.Column="0" Grid.Row="0" Grid.ColumnSpan="2" Height="20" HorizontalAlignment="Right" VerticalAlignment="Center" Width="30" Background="black" Margin="0,1,10,-0.2" />
                            <TextBlock FontSize="14" Foreground="White" Grid.ColumnSpan="2" Margin="18,-1,2,0.6"><Run Text="A"/><Run BaselineAlignment="Superscript" FontSize="8" Text="+"/></TextBlock>
                            <Polygon Grid.Row="0" HorizontalAlignment="Left" VerticalAlignment="Center" Margin="40,2,0,-0.2" Points="0,17.9,0,0,10,10" Fill="Black" Stroke="Black" Grid.ColumnSpan="2"/>
                        </Grid>
                    </Canvas>
